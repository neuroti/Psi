# Psi Backend Environment Variables

# API Settings
SECRET_KEY=your-secret-key-here-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_HOURS=24

# PostgreSQL Database
POSTGRES_SERVER=localhost
POSTGRES_USER=psi_user
POSTGRES_PASSWORD=your_password_here
POSTGRES_DB=psi_db

# MongoDB
MONGODB_URL=mongodb://localhost:27017
MONGODB_DB=psi

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# External APIs
CLAUDE_API_KEY=your-claude-api-key
USDA_API_KEY=

# YOLO Model
YOLO_MODEL_PATH=data/models/psi_food_best.pt
YOLO_CONFIDENCE_THRESHOLD=0.5
YOLO_HIGH_CONFIDENCE_THRESHOLD=0.8

# AWS S3 (Optional)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET=psi-images
AWS_REGION=us-east-1

# CORS Origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:3000,exp://localhost:19000

# Rate Limiting
RATE_LIMIT_PER_MINUTE=100
FREE_TIER_DAILY_LIMIT=3
